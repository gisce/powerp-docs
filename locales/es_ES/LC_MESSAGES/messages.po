msgstr "Fichero: <strong>F1_YYYY_AAAAMMDD_aaaammdd.v</strong>"

msgid ""
"<strong>YYYY:</strong> Codi de participant, valor indicat en el camp \n"
"<strong>Còdigo R1</strong>."
msgstr ""
"<strong>YYYY:</strong> Código de participante, valor indicado en el campo \n"
"<strong>Código R1</strong>."

msgid "<strong>AAAAMMDD:</strong> Data a la que es corresponen les dades (Columna 1 arxiu TPL)."
msgstr "<strong>AAAAMMDD:</strong> Fecha a la que corresponden los datos (Columna 1 archivo TPL)."

msgid "<strong>aaaammdd:</strong> Data de generació del fitxer (data actual)."
msgstr "<strong>aaaammdd:</strong> Fecha de generación del archivo (fecha actual)."

